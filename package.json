{
  "name": "g0-gpm-cli",
  "version": "0.1.0",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "g0": "./bin/g0"
  },
  "scripts": {
    "prebuild": "node scripts/bundle-resources.js",
    "build": "npm run prebuild && tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "commander",
    "typescript",
    "g0",
    "gpm"
  ],
  "author": "",
  "license": "ISC",
  "description": "A basic CLI tool built with Commander and TypeScript",
  "dependencies": {
    "commander": "^14.0.0",
    "gray-matter": "^4.0.3",
    "hash-wasm": "^4.12.0",
    "js-yaml": "^4.1.0",
    "junk": "^4.0.1",
    "prompts": "^2.4.2",
    "semver": "^7.7.2",
    "tar": "^6.2.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.3.1",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.7.1",
    "@types/tar": "^6.1.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
