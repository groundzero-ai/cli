{
  "name": "g0-gpm-cli",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "g0": "./bin/g0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["cli", "commander", "typescript", "g0", "gpm"],
  "author": "",
  "license": "ISC",
  "description": "A basic CLI tool built with Commander and TypeScript",
  "dependencies": {
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/node": "^24.3.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
